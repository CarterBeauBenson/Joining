                                                                                                                                                 <https://purl.industrialontologies.org/ontology/core/Core/EngineeredSystem>
                                                                                                                                                 <https://purl.industrialontologies.org/ontology/core/Core/Person>
                                                                                                                                               )
                                                                                                                                 ]
                                                                                                                                 [ rdf:type owl:Restriction ;
                                                                                                                                   owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000056> ;
