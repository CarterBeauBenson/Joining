                                                                                               owl:someValuesFrom [ owl:intersectionOf ( <https://purl.industrialontologies.org/ontology/core/Core/Agent>
                                                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                                                           owl:onProperty <https://purl.industrialontologies.org/ontology/core/Core/hasRole> ;
                                                                                                                                           owl:someValuesFrom <https://purl.industrialontologies.org/ontology/core/Core/SupplierRole>
                                                                                                                                         ]
                                                                                                                                       ) ;
                                                                                                                    rdf:type owl:Class
                                                                                                                  ]
                                                                                             ] ,
                                                                                             [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000117> ;
                                                                                               owl:someValuesFrom <https://purl.industrialontologies.org/ontology/core/Core/SupplyingBusinessProcess>
                                                                                             ] ,
                                                                                             [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <https://purl.industrialontologies.org/ontology/core/Core/isSubjectOf> ;
